{
  "name": "LSB Wallet",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Light Shadow Box - Wallet Extension",
  "icons": {
    "128": "logo.png"
  },
  "permissions": [
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "index.html"
  },
  "options_page": "index.html"
}
